{
    "name": "kodepintar/laravel-rajaongkir",
    "description": "RajaOngkir wrapper for Laravel",
    "version": "2.0.0",
    "keywords": [
        "kodepintar",
        "laravel",
        "laravel-rajaongkir",
        "rajaongkir"
    ],
    "homepage": "https://github.com/kodepintar/laravel-rajaongkir",
    "license": "MIT",
    "authors": [
        {
            "name": "Tio",
            "email": "sangvictim@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/config": "^8",
        "illuminate/support": "^8"

    },
    "require-dev": {
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Kodepintar\\LaravelRajaOngkir\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kodepintar\\LaravelRajaOngkir\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kodepintar\\LaravelRajaOngkir\\RajaOngkirServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
